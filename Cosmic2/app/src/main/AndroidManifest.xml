<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.usurio.cosmic">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>
        </activity>

        <activity
            android:name=".Selectors"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".Planets"
            android:parentActivityName=".Selectors"></activity>
        <activity
            android:name=".Estrelas"
            android:parentActivityName=".Selectors"></activity>
        <activity
            android:name=".Missoes"
            android:parentActivityName=".Selectors"></activity>
        <activity
            android:name=".Relatos"
            android:parentActivityName=".Selectors"></activity>
        <activity
            android:name=".Satelite"
            android:parentActivityName=".Selectors"></activity>
        <activity
            android:name=".Images"
            android:parentActivityName=".Selectors"></activity>
    </application>

</manifest>
